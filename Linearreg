lm_mod1 <- read.csv("C:/Users/Shilpa/Desktop/SDM/final/EMI/final_reg1.csv")
View(lm_mod1)
lm<- lm(RATING~., data=lm_mod1) # linear regression with kitchen sink model
summary(lm)
lm1<-lm(RATING~TRAIN_USER+TRAIN_ARTIST+HEARD_OF+OWN_ARTIST_MUSIC+LIKE_ARTIST+AGGRESSIVE+SERIOUS+UNATTRACTIVE+CONFIDENT+BORING+
          CURRENTS+STYLISH+CHEAP+IRRELEVANT+OUTGOING+INSPIRING+BEAUTIFUL+FUN+AUTHENTIC+COOL+CATCHY+SUPERFICIAL+ANNOYING+PASSIONATE+GOOD_LYRICS+
          TIMELESS+DEPRESSING+ORIGINAL+TALENTED+DISTINCTIVE+ APPROACHABLE+NOISY+ENERGETIC+NONE+SEXY+OVER+CHEESY+UNORIGINAL+DATED+
          CLASSIC+WARM+GENDER+AGE+WORKING+MUSIC+LIST_OWN+Q4+Q7+Q8+Q11+Q16+Q17+TRACK+TIME,data=lm_mod1)
#Linear regression after removing insignificant variables
summary(lm1)

#Logistic Regression
lm2<-glm(RATING~.,data=lm_mod1)
summary(lm2)
lm3<-glm(RATING~TRAIN_USER+TRAIN_ARTIST+HEARD_OF+OWN_ARTIST_MUSIC+LIKE_ARTIST+AGGRESSIVE+SERIOUS+UNATTRACTIVE+CONFIDENT+BORING+
          CURRENTS+STYLISH+CHEAP+IRRELEVANT+OUTGOING+INSPIRING+BEAUTIFUL+FUN+AUTHENTIC+COOL+CATCHY+SUPERFICIAL+ANNOYING+PASSIONATE+GOOD_LYRICS+
          TIMELESS+DEPRESSING+ORIGINAL+TALENTED+DISTINCTIVE+ APPROACHABLE+NOISY+ENERGETIC+NONE+SEXY+OVER+CHEESY+UNORIGINAL+DATED+
          CLASSIC+WARM+GENDER+AGE+WORKING+MUSIC+LIST_OWN+Q4+Q7+Q8+Q11+Q16+Q17+TRACK+TIME,data=lm_mod1,family="binomial")
summary(lm3)
